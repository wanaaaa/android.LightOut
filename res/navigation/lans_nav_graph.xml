<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/lan_navigation_game">


    <fragment
        android:id="@+id/lan_navigation_game"
        android:name="com.zybooks.lightsoutnav.lands_game"
        android:label="@string/title_game"
        tools:layout="@layout/fragment_lands_game" />

    <fragment
        android:id="@+id/navigation_color"
        android:name="com.zybooks.lightsoutnav.ColorFragment"
        android:label="@string/title_color"
        tools:layout="@layout/fragment_color" />

    <fragment
        android:id="@+id/navigation_help"
        android:name="com.zybooks.lightsoutnav.HelpFragment"
        android:label="@string/title_help"
        tools:layout="@layout/fragment_help" />

</navigation>






